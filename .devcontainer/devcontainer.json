{
  "name": "Gold Commodity Exchange",
  "dockerComposeFile": "../docker-compose.yml",
  "service": "frontend",
  "workspaceFolder": "/workspaces/Gold-commodity-exchange",
  "postCreateCommand": "docker compose up -d",
  "forwardPorts": [3000],
  "customizations": {
    "vscode": {
      "extensions": ["ms-azuretools.vscode-docker"]
    }
  },
  "portsAttributes": {
    "3000": {
      "label": "Frontend",
      "onAutoForward": "openPreview"
    }
  }
}
