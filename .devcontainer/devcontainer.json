{
  "name": "Gold Commodity Exchange",
  "dockerComposeFile": "docker-compose.yml",
  "service": "frontend",
  "workspaceFolder": "../",
  "postCreateCommand": "docker-compose up -d",
  "forwardPorts": [3000],
  "customizations": {
    "vscode": {
      "extensions": ["ms-azuretools.vscode-docker"]
    }
  },
  "remoteUser": "vscode",
  "portsAttributes": {
    "3000": {
      "label": "Frontend",
      "onAutoForward": "openPreview"
    }
  }
}
